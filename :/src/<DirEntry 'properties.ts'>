export const getHttpUrl = (apiName: any) => {
    const endPoint = endPoints[apiName]
    const url = baseURL + endPoint;
    return url;
};

export const getDplURL = (apiName: any) => {
    const endPoint = endPoints[apiName]
    const url = dplBaseUrl + endPoint;
    return url;
};

export const getExternalBaseUrl = (apiName: any) => {
    const endPoint = endPoints[apiName]
    const url = externalBaseUrl + endPoint;
    return url;
};

export const getPurchaseUrl = (apiName: any) => {
    const endPoint = endPoints[apiName]
    const url = purchaseOrderUrl + endPoint;
    return url;
};

export const getControledAppUrl = (apiName: any) => {
    const endPoint = endPoints[apiName]
    const url = controlAppsUrl + endPoint;
  
    return url;
};



